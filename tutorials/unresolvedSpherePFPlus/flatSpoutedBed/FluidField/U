FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet_left
    {
        type            fixedValue;
        value           uniform (0.0 2.0 0);
    }

    inlet_middle
    {
        type            uniformFixedValue;
        uniformValue    table
        (
            (1.2     (0 0 0))
            (1.3     (0 43.5 0))
        );
    }

    inlet_right
    {
        type            fixedValue;
        value           uniform (0.0 2.0 0);
    }

    defaultFaces
    {
        type            noSlip;
        value           uniform (0 0 0);
    }

    outlet
    {
        type            zeroGradient;
    }
}

