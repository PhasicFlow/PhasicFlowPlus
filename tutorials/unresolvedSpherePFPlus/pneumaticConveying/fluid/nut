FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-9;

boundaryField
{

    top
    {
        type            nutkWallFunction;
        value           uniform 1e-5;
    }

    bottom
    {
        type            nutkWallFunction;
        value           uniform 1e-5;
    }

    inlet
    {
        type            cyclic;
        value           $internalField;
    }

    outlet
    {
        type            cyclic;
    }

    rear
    {
        type            nutkWallFunction;
        value           uniform 1e-5;
    }

    front
    {
        type            nutkWallFunction;
        value           uniform 1e-5;
    }
}


// ************************************************************************* //
