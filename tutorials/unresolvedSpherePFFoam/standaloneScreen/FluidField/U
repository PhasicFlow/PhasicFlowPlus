FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
wall
    {
        type            noSlip;
    }

    inlet
    {
        type           fixedValue;
        value           uniform (0 -0.04 0);
    }
      
  outlet
    {
        type             pressureInletOutletVelocity;
        inletValue 	 uniform (0 0 0);
        value 		 uniform (0 0 0);
    wall-drawing1.dwg_drawing1.dwg
    {
        type            noSlip;
    }
  
    
}
