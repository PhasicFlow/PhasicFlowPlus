FoamFile
{
    format      ascii;
    class       volScalarField;
    object      alpha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type fixedValue;
        value uniform 1;
    }
    outlet
    {
        type zeroGradient;
    }
    fixedWalls
    {
        type zeroGradient;
    }
}

